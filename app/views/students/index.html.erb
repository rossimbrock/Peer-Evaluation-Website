<p style="color: green"><%= notice %></p>

<h1>Students</h1>

<div id="students">
  <% @students.each do |student| %>
    <p>
      <%= link_to student[:first_name].to_s+" "+student[:last_name].to_s, student %>
    </p>
  <% end %>
</div>

<%= link_to "New student", new_student_path %>
